programa
{
	funcao inicio ()
	{
	    inteiro numero = 0, contador = 1
	    
	    escreva("Digite um nÃºmero entre 1 e 10: ")
	    leia(numero)
	    
	    se (numero >= 1 e numero <= 10) {
	        enquanto (contador <= 10) {
	            escreva(numero, " x ", contador, " = ", numero * contador, "\n")
	            
	            contador = contador + 1
	        }
	    }
	}
}
